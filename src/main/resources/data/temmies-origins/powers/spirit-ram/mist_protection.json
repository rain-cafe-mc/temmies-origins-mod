{
    "name":"Mist Protection",
    "description":"You can summon a big mist cloud around you that starts to suffocate everything around you. [Secondary]",
    "type":"origins:multiple",
    "create_mist_tag":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:execute_command",
          "command":"tag @s add mist",
          "permission_level":4
       },
       "interval":1
    },
    "create_stop_tag":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:execute_command",
          "command":"tag @e[tag=!rotten,distance=..20] add stop",
          "permission_level":4
       },
       "interval":1
    },
    "mist_on":{
       "type":"origins:resource",
       "max":1,
       "min":0,
       "hud_render":{
          "should_render":false,
          "sprite_location":"origins:textures/gui/community/spiderkolo/resource_bar_01.png",
          "bar_index":9,
          "condition":{
             "type":"origins:resource",
             "resource":"temmies-origins:spirit-ram/mist_protection_mist_on",
             "comparison":">",
             "compare_to":0
          }
       }
    },
    "mac":{
       "type":"origins:resource",
       "max":500,
       "min":0,
       "hud_render":{
          "should_render":true,
          "sprite_location":"origins:textures/gui/community/spiderkolo/resource_bar_01.png",
          "bar_index":9,
          "condition":{
             "type":"origins:resource",
             "resource":"temmies-origins:spirit-ram/mist_protection_mac",
             "comparison":">",
             "compare_to":0
          }
       }
    },
    "mist_effects":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:and",
          "actions":[
             {
                "type":"origins:execute_command",
                "command":"effect give @e[tag=!mist,tag=stop,distance=..12] minecraft:weakness 1 5 true",
                "permission_level":4
             },
             {
                "type":"origins:execute_command",
                "command":"effect give @e[tag=!mist,tag=stop,distance=..12] minecraft:wither 1 3 true",
                "permission_level":4
             },
             {
                "type":"origins:execute_command",
                "command":"effect give @e[tag=!mist,tag=stop,distance=..12] minecraft:slowness 1 3 true",
                "permission_level":4
             }
          ]
       },
       "interval":1,
       "condition":{
          "type":"origins:resource",
          "resource":"temmies-origins:spirit-ram/mist_protection_mist_on",
          "comparison":"==",
          "compare_to":1
       }
    },
    "particles":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:execute_command",
          "command":"minecraft:cloud ~ ~ ~ 8 8 8 0.55 1000"
       },
       "interval":15,
       "condition":{
          "type":"origins:and",
          "conditions":[
             {
                "type":"origins:resource",
                "resource":"temmies-origins:spirit-ram/mist_protection_mac",
                "comparison":"<",
                "compare_to":200
             },
             {
                "type":"origins:resource",
                "resource":"temmies-origins:spirit-ram/mist_protection_mist_on",
                "comparison":"==",
                "compare_to":1
             }
          ]
       }
    },
    "hjdfjs":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:and",
          "actions":[
             {
                "type":"origins:delay",
                "ticks":8,
                "action":{
                   "type":"origins:execute_command",
                   "command":"execute at @e[tag=stop,tag=!mist,type=!item,distance=..10] run particle minecraft:cloud ~ ~1 ~ .35 .35 .35 0.8 25 normal",
                   "permission_level":4
                }
             }
          ]
       },
       "interval":1,
       "condition":{
          "type":"origins:resource",
          "resource":"temmies-origins:spirit-ram/mist_protection_mist_on",
          "comparison":"==",
          "compare_to":1
       }
    },
    "up_ig":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:change_resource",
          "resource":"temmies-origins:spirit-ram/mist_protection_mac",
          "change":1
       },
       "interval":1,
       "condition":{
          "type":"origins:and",
          "conditions":[
             {
                "type":"origins:resource",
                "resource":"temmies-origins:spirit-ram/mist_protection_mac",
                "comparison":"<",
                "compare_to":500
             },
             {
                "type":"origins:resource",
                "resource":"temmies-origins:spirit-ram/mist_protection_mac",
                "comparison":">",
                "compare_to":0
             }
          ]
       }
    },
    "down_ig":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:and",
          "actions":[
             {
                "type":"origins:change_resource",
                "resource":"temmies-origins:spirit-ram/mist_protection_mist_on",
                "change":-1
             }
          ]
       },
       "interval":1,
       "condition":{
          "type":"origins:resource",
          "resource":"temmies-origins:spirit-ram/mist_protection_mac",
          "comparison":"==",
          "compare_to":200
       }
    },
    "down_igd":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:and",
          "actions":[
             {
                "type":"origins:delay",
                "ticks":5,
                "action":{
                   "type":"origins:change_resource",
                   "resource":"temmies-origins:spirit-ram/mist_protection_mac",
                   "change":-500
                }
             }
          ]
       },
       "interval":1,
       "condition":{
          "type":"origins:resource",
          "resource":"temmies-origins:spirit-ram/mist_protection_mac",
          "comparison":"==",
          "compare_to":500
       }
    }
}