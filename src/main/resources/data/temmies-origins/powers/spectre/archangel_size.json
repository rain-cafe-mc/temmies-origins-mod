{
	"type":"origins:action_on_callback",
	"name": "Archangel",
	"description": "As a former Archangel, you are slightly larger and have longer arms.",
	"execute_chosen_when_orb":true,
	"entity_action_chosen": {
	   "type":"origins:and",
	   "actions":[
		  {
			 "type":"origins:execute_command",
			 "command":"scale set pehkui:height 1.1 @s",
			 "permission_level":4
		  },
		  {
			 "type":"origins:execute_command",
			 "command":"scale set pehkui:width 1.1 @s",
			 "permission_level":4
		  }
	   ]
	},
	"entity_action_respawned": {
	   "type":"origins:and",
	   "actions":[
		  {
			 "type":"origins:execute_command",
			 "command":"scale set pehkui:height 1.1 @s",
			 "permission_level":4
		  },
		  {
			 "type":"origins:execute_command",
			 "command":"scale set pehkui:width 1.1 @s",
			 "permission_level":4
		  },
		  {
			 "type":"origins:execute_command",
			 "command":"resource set @s petertp:spectre_infinite 0",
			 "permission_level":4
		  }
	   ]
	},
	"entity_action_lost": {
	   "type":"origins:and",
	   "actions":[
		  {
			 "type":"origins:execute_command",
			 "command":"scale set pehkui:height 1 @s",
			 "permission_level":4
		  },
		  {
			 "type":"origins:execute_command",
			 "command":"scale set pehkui:width 1 @s",
			 "permission_level":4
		  }
	   ]
	}
}