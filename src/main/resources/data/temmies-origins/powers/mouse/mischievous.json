{
    "name": "Mischievous",
    "description": "Mouses are quick, you can travel greater distances in a short amount of time. [Primary]",
    "type":"origins:active_self",
	"key":"key.origins.primary_active",
	"cooldown":500,
    "hud_render":{
       "should_render":true
    },
    "entity_action":{
       "type":"origins:and",
       "actions":[
          {
             "type":"origins:raycast",
             "distance":13,
             "entity":false,
             "before_action":{
                "type":"origins:and",
                "actions":{
                   "type":"origins:execute_command",
                   "command":"tag @s add tping"
                }
             },
             "command_step":1,
             "command_along_ray":"execute positioned ~ ~ ~ if block ~ ~ ~ air run tp @e[tag=tping,sort=nearest] ~ ~ ~",
             "miss_action":{
                "type":"origins:and",
                "actions":[
                   {
                      "type":"origins:add_velocity",
                      "space":"local",
                      "z":0.1
                   },
                   {
                      "type":"origins:execute_command",
                      "command":"tag @s remove tping"
                   }
                ]
             },
             "hit_action":{
                "type":"origins:and",
                "actions":[
                   {
                      "type":"origins:add_velocity",
                      "space":"local",
                      "z":0.1
                   },
                   {
                      "type":"origins:execute_command",
                      "command":"tag @s remove tping"
                   }
                ]
             }
          }
       ]
    }
}