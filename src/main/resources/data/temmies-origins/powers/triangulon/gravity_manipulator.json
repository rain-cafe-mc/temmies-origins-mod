{
    "name": "Gravity Manipulator",
    "description": "You can fly, have void immunity, and can walk on liquids.",
	"type": "origins:multiple",
	"flight": {
		"type": "origins:creative_flight",
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_timer",
			"comparison": ">",
			"compare_to": 1
		}
	},
	"timer": {
		"type": "origins:resource",
		"min": 0,
		"max": 400,
		"hud_render": {
			"bar_index": 4
		}
	},
	"countdown": {
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:creative_flying"
			},
			"if_action": {
				"type": "origins:change_resource",
				"resource": "*:*_timer",
				"change": -2
			},
			"else_action": {
				"type": "origins:change_resource",
				"resource": "*:*_timer",
				"change": 1
			}
		},
		"interval": 2
	}
}